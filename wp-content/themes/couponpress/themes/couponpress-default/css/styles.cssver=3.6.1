#header { display:none; }
.itembox {border: 0px;clear: both;margin: 0px 0px 0px;padding: 0px;}
#header, .menu {margin-left: 0px;}
#submenu_li_login, #submenu_li_register, #submenu_li_account, #submenu_li_logout {text-shadow: 1px 1px 1px #666;}

 /* =============================================================================
