   ========================================================================== */
 
#logo { width:400px; margin-left: -10px; } 
#banner { width:540px  }
#banner img { float:right; }
body {	background: #8b84b7 url(../images/bg.png) repeat;	min-height: 100%;	padding-bottom: 1px;}
a { color:#8B84B7; } 
#content {background: white;padding-top: 0px !important;} 
.menu {margin-top:2px;margin-bottom:0px;padding: 5px; background: #dedbef;height: 100px;}
.menu li a {font-size: 14px;font-weight: bold;color: #fff;text-shadow: 1px 1px #577522;line-height: 52px;padding-top:0px;}
.sub-menu li a { line-height:25px; }
.menu li {background: url(../images/nav-sep.png) no-repeat top right}
.menu .w_960 { background: #7c76a3 url(../images/nav-gradient.png) repeat-x top; height: 52px; width:950px;  }
.accountbar li { background:none; }
.accountbar li a {float:left; width:60px; font-size:12px; }

.introtxt { line-height:55px; float:left; font-size:16px; font-style:italic; margin-left:10px;  color:#8b84b7;   text-shadow: 1px 1px #fff;  }

#submenu_li_login, #submenu_li_logout, #submenu_li_account { background:url('../images/i1.png') 0px 20px no-repeat; padding-left:5px; }
#submenu_li_register { background:url('../images/i1.png') 0px 20px no-repeat; padding-left:15px; }
#custom-search-form {  float: right; padding-top:10px; margin-right:10px; }
#custom-search-form .nice_search {line-height: 12px;width: 350px;color: #ccc;margin: 0;padding: 9px;border: 0;background: #f2f2f2;}
#submenu_li_logout { min-width:80px !important; width:80px !important; }
#submenu_li_account a { width:100px; }
#custom-search-form .nice_submit { background: transparent url(../images/button-search.png) no-repeat right;  width:30px; border:0px; cursor:pointer; }
.homeli {width: 30px !important;display: block;text-indent: -9999px;background: url(../images/nav-home.png) no-repeat 7px 20px;}
 
.wrapper {padding: 10px;border:5px solid #7c76a3;background:  #fff;}
#sidebar .sidebar_wrapper { margin-bottom: 10px;border: 5px solid #F2F2F2;background: #F7F7F7;}
#sidebar .itembox { padding:5px;  }
#single-relatedcoupons {background: none;margin: 0px;padding: 0px;border-bottom: 1px dotted #CCC;margin-bottom: 10px;margin-top: 20px;}

#sidebar .greybg, #sidebar-left .greybg { background:none; }
#sidebar h2, #PPTHomePage h2  {padding: 10px 15px;margin: -5px -5px 15px -5px;background: #F2F2F2;font-weight: bold;font-size: 20px;color: #7c76a3;}
#PPTHomePage .middle2cols h2, .itembox h1, #icon-single-title { background:none; margin:0px; padding:0px; border-bottom:1px dotted #ccc; margin-bottom:10px;}

.middle2cols .itembox h1 { color: #7C76A3; }
 
#homeCompactCategories {display: block;clear: both;overflow: hidden;padding: 10px 0 10px 10px;margin-bottom: 10px;background: #F7F7F7;border: 5px solid #F2F2F2; z-index: 5;}

#homeCompactCategories a { color:#666; }

.CouponPopList li { border:0px; padding:0px; }
.CouponPopList li a { background:url(../images/i1.png) 5px 7px no-repeat; }
 #PPTHomePage #style2_wrapper { background: #F7F7F7;border: 5px solid #F2F2F2;z-index: 5;}

#icon-home-cats { display:none; }
ul.pagination { margin-top:10px; }
.middle2cols #ppt-widget-stores { margin-bottom:0px; }
.middle2cols #ppt-widget-stores-box {padding-top: 0px; background:#fff; } 

#PPTGalleryPage .padbox {padding: 0px; padding-bottom:10px; }
#PPTGalleryPage .lefttxt {line-height: 25px;}

#home-recentadded { margin-top:10px !important; }

.couponinfo, .article .date {	height:25px;	margin-bottom:10px;	color:#fff;	font:11px Arial, Helvetica, sans-serif;}
.couponinfo ul, .article .date ul {	margin:0px;	list-style:none;}
.couponinfo ul li, .article .date ul li{	color:#666; float:left;	margin-right:5px;	line-height:25px;	height:25px;	-webkit-border-radius:3px;	-moz-border-radius: 3px;	border-radius: 3px;	background-color:#eeeeee;}
.couponinfo ul li.datepad, .article .date ul li.datepad {	padding:0 10px;}
.couponinfo ul li a, .article .date ul li a {	float:left;	height:25px;	color:#666;	padding:0 10px;	display:block;	-webkit-border-radius:3px;	-moz-border-radius:3px;	border-radius: 3px; text-decoration:none;}
.couponinfo ul li a:hover, .article .date ul li a:hover {	background-color:#ccc;	text-decoration:none;}
.couponinfo .post-categories li {	margin-right:0;}


 
	#PPTHomePage #style1 .next_button, #PPTHomePage #style2 .next_button, #PPTHomePage #style3 .next_button {
	background:url('../images/sliderb1.png') no-repeat; margin-top:60px; height:35px; width:35px;
	}
	
	#PPTHomePage #style1 .previous_button, #PPTHomePage #style2 .previous_button, #PPTHomePage #style3 .previous_button  {
